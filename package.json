{
  "name": "leaflet.tilelayer.pouchdbcached",
  "version": "1.0.1",
  "description": "Allows all TileLayers to cache into PouchDB for offline use",
  "scripts": {
    "build": "rollup -c rollup.config.esm.js && rollup -c rollup.config.iife.js",
    "build:esm": "rollup -c rollup.config.esm.js"
  },
  "type": "module",
  "main": "dist/L.TileLayer.PouchDBCached.esm.js",
  "exports": {
    "import": "./dist/L.TileLayer.PouchDBCached.esm.js"
  },
  "files": [
    "dist/",
    "L.TileLayer.PouchDBCached.js"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MrRedBeard/Leaflet.TileLayer.PouchDBCached.git"
  },
  "keywords": [
    "leaflet",
    "pouchdb"
  ],
  "author": "MrRedBeard and Iván Sánchez Ortega <ivan@mazemap.no>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MrRedBeard/Leaflet.TileLayer.PouchDBCached/issues"
  },
  "homepage": "https://github.com/MrRedBeard/Leaflet.TileLayer.PouchDBCached#readme",
  "dependencies": {
    "leaflet": "^1.9.4",
    "pouchdb": "^9.0.0",
    "pouchdb-browser": "^9.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "rollup": "^4.45.0",
    "rollup-plugin-node-polyfills": "^0.2.1"
  }
}
